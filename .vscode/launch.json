    "version": "0.2.0",
    "configurations": [
        {
            "name": "train",
            "type": "python",
            "request": "launch",
            "program": "E:/QuantumFold/cnn/train.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "args": [
                "--auxiliary",
                "--cutout",
                "--arch=G_C_20",
                "--gpu=0"
            ], 
        },
        {
            "name": "train_imagenet",
            "type": "python",
            "request": "launch",
            "program": "E:/QuantumFold/cnn/train_imagenet.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        },
        {
            "name": "search",
            "type": "python",
            "request": "launch",
            "program": "E:/QuantumFold/cnn/search_darts.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "args": [
                "--layers=8",
                "--primitive=p2",
                "--batch_size=32",
                "--load_workers=0",
            ],
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        }
    ]
}